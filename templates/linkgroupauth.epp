<% | Hash $template |-%>
# Managed by Puppet.
# Any modifications will be lost with the next Pupet synchronization run!

<% each(sort(keys($template))) |$group| { -%>
  <%- $logins = $template[$group] -%>
linkGroup <%= $group %>
  <%- validate_array($logins) -%>
  <%- each($logins) |$login| { -%>
  <%= $login %>
  <%- } -%>

<% } %>